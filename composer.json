{
    "name": "sarala-io/sarala",
    "type": "library",
    "description": "Laravel RESTful API Companion",
    "keywords": [
        "sarala-io",
        "sarala",
        "RESTful",
        "REST",
        "API"
    ],
    "homepage": "https://github.com/sarala-io/sarala",
    "license": "MIT",
    "authors": [
        {
            "name": "Milroy E. Fraser",
            "email": "milroy.me@gmail.com",
            "homepage": "https://milroy.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/http": "^6.0|^7.0|^8.0",
        "league/fractal": "^0.19.2"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sarala\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sarala\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sarala\\SaralaServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
